<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/spacing_xnomarl"/>
            <solid android:color="@color/color_primary_select_bg"/>
            <stroke android:color="@color/transparent" android:width="0.5dp"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/spacing_xnomarl"/>
            <solid android:color="@color/white"/>
            <stroke android:color="@color/transparent" android:width="0.5dp"/>
        </shape>
    </item>
</selector>